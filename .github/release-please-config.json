    },
    "tsconfig": {
      "component": "@snipkit/tsconfig",
      "skip-github-release": true
    }
  },
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "snipkit-js",
      "components": [
        "root",
        "@snipkit/analyze",
        "@snipkit/analyze-wasm",
        "snipkit",
        "@snipkit/astro",
        "@snipkit/bun",
        "@snipkit/deno",
        "@snipkit/nest",
        "@snipkit/next",
        "@snipkit/node",
        "@snipkit/remix",
        "@snipkit/sveltekit",
        "@snipkit/decorate",
        "@snipkit/duration",
        "@snipkit/env",
        "@snipkit/eslint-config",
        "@snipkit/headers",
        "@snipkit/inspect",
        "@snipkit/ip",
