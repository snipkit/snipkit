{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "versioning": "prerelease",
  "release-type": "node",
  "changelog-path": "CHANGELOG.md",
  "changelog-sections": [
    { "type": "feat", "section": "üöÄ New Features" },
    { "type": "fix", "section": "ü™≤ Bug Fixes" },
    { "type": "perf", "section": "üèéÔ∏è Performance Improvements" },
    { "type": "revert", "section": "‚Ü©Ô∏è Reverts" },
    { "type": "docs", "section": "üìù Documentation" },
    { "type": "chore", "section": "üßπ Miscellaneous Chores" },
    { "type": "refactor", "section": "‚å®Ô∏è Code Refactoring" },
    { "type": "test", "section": "üìö Tests" },
    { "type": "build", "section": "üî® Build System" },
    { "type": "ci", "section": "‚úÖ Continuous Integration" }
  ],
  "group-pull-request-title-pattern": "chore: Release ${version}",
  "pull-request-header": "Automatically generated with [Release Please](https://github.com/googleapis/release-please).",
  "pull-request-footer": "",
  "include-v-in-tag": true,
  "bump-minor-pre-major": false,
  "bump-patch-for-minor-pre-major": false,
  "draft": false,
  "prerelease": false,
  "packages": {
    ".": {
      "component": "root",
      "include-component-in-tag": false
    },
    "analyze": {
      "component": "@snipkit/analyze",
      "skip-github-release": true
    },
    "analyze-wasm": {
      "component": "@snipkit/analyze-wasm",
      "skip-github-release": true
    },
    "snipkit": {
      "component": "snipkit",
      "skip-github-release": true
    },
    "snipkit-astro": {
      "component": "@snipkit/astro",
      "skip-github-release": true
    },
    "snipkit-bun": {
      "component": "@snipkit/bun",
      "skip-github-release": true
    },
    "snipkit-deno": {
      "component": "@snipkit/deno",
      "skip-github-release": true
    },
    "snipkit-nest": {
      "component": "@snipkit/nest",
      "skip-github-release": true
    },
    "snipkit-next": {
      "component": "@snipkit/next",
      "skip-github-release": true
    },
    "snipkit-node": {
      "component": "@snipkit/node",
      "skip-github-release": true
    },
    "snipkit-remix": {
      "component": "@snipkit/remix",
      "skip-github-release": true
    },
    "snipkit-sveltekit": {
      "component": "@snipkit/sveltekit",
      "skip-github-release": true
    },
    "body": {
      "component": "@snipkit/body",
      "skip-github-release": true
    },
    "decorate": {
      "component": "@snipkit/decorate",
      "skip-github-release": true
    },
    "duration": {
      "component": "@snipkit/duration",
      "skip-github-release": true
    },
    "env": {
      "component": "@snipkit/env",
      "skip-github-release": true
    },
    "eslint-config": {
      "component": "@snipkit/eslint-config",
      "skip-github-release": true
    },
    "headers": {
      "component": "@snipkit/headers",
      "skip-github-release": true
    },
    "inspect": {
      "component": "@snipkit/inspect",
      "skip-github-release": true
    },
    "ip": {
      "component": "@snipkit/ip",
      "skip-github-release": true
    },
    "logger": {
      "component": "@snipkit/logger",
      "skip-github-release": true
    },
    "nosecone": {
      "component": "nosecone",
      "skip-github-release": true
    },
    "nosecone-next": {
      "component": "@nosecone/next",
      "skip-github-release": true
    },
    "nosecone-sveltekit": {
      "component": "@nosecone/sveltekit",
      "skip-github-release": true
    },
    "protocol": {
      "component": "@snipkit/protocol",
      "skip-github-release": true
    },
    "redact": {
      "component": "@snipkit/redact",
      "skip-github-release": true
    },
    "redact-wasm": {
      "component": "@snipkit/redact-wasm",
      "skip-github-release": true
    },
    "rollup-config": {
      "component": "@snipkit/rollup-config",
      "skip-github-release": true
    },
    "runtime": {
      "component": "@snipkit/runtime",
      "skip-github-release": true
    },
    "sprintf": {
      "component": "@snipkit/sprintf",
      "skip-github-release": true
    },
    "stable-hash": {
